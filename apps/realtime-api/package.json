{
  "name": "realtime-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "db:migrate": "dotenv -e ../../.env -- node-pg-migrate up -m ../../db/migrations",
    "db:rollback": "dotenv -e ../../.env -- node-pg-migrate down -m ../../db/migrations",
    "db:seed": "dotenv -e ../../.env -- tsx scripts/seed.ts",
    "test:call": "tsx scripts/simulate-call.ts",
    "lint": "echo \"Lint not configured yet\""
  },
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/websocket": "^8.3.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.1",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.104.0",
    "pg": "^8.11.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.3",
    "@types/ws": "^8.18.1",
    "dotenv-cli": "^11.0.0",
    "node-pg-migrate": "^7.6.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "ws": "^8.18.3"
  }
}
